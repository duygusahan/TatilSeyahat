@model List<TatilSeyahat.Models.Siniflar.Blog>
<div class="padding1">
    <h4>En Beğendiğim Yerler</h4>
    <div class="box-left">
        @foreach (var item in Model)
        {
            <div class="bot-img" >
                <img src="@item.ImgUrl" alt="" style="width:120px ; height:65px"/>
            </div>
            <div class="bot-desc">
                <p>@item.Aciklama.Substring(0, 65)...</p>
            </div>
            <div class="clear"></div>
        }
    </div>
    @Html.Action("_PartialBestPlaceRightSide", "Default")

</div>
